int main() {
#ifndef ONLINE_JUDGE
	freopen("Error.txt", "w", stderr);
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
#endif
ll n;cin>>n;
cout<<n<<" ";
while(n!=1){
	if(n%2){
		n=n*3+1;
	}
	else{
		n/=2;
	}
	cout<<n<<" ";
}
 
}
